package com.modofo.mofire.domain;

import com.modofo.mofire.Constants;

public class ImageAttachment {
	private int from;//1-local file,2-in memory(capture by internal cam)
	private String type = Constants.ATTACHMENT_TYPE_IMAGE; 
	private String filePath; //if the image from local gallery
	private byte[] byteData;
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public String getFilePath() {
		return filePath;
	}
	public void setFilePath(String filePath) {
		this.filePath = filePath;
	}
	public byte[] getByteData() {
		return byteData;
	}
	public void setByteData(byte[] byteData) {
		this.byteData = byteData;
	}
	public int getFrom() {
		return from;
	}
	public void setFrom(int from) {
		this.from = from;
	}
	
}
